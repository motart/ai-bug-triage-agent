# Sample environment configuration for ai-bug-triage-agent
# Copy this file to .env and fill in the values for your environment

# Jira settings
JIRA_URL=
JIRA_USER=
JIRA_TOKEN=
JIRA_PROJECT=

# Version control settings
# Set VCS_TYPE to "git" for GitHub or "p4" for Perforce
VCS_TYPE=git

# GitHub settings
GITHUB_REPO=
GITHUB_TOKEN=

# Perforce settings
P4PORT=
P4USER=
P4TICKET=

# Terraform settings
TERRAFORM_DIR=
